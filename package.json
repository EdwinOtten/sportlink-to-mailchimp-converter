    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.13.0",
    "jest": "^29.7.0",
    "rollup": "^4.24.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.5.4"
